; Parameters for the framework
[parameters]

; App Name
app.name           = <%= @app[:name] %>
app.normalizedName = <%= @app[:normalizedName] %>

; assetic.xml
assetic.yuicompressor.jar = %kernel.root_dir%/../vendor/nervo/yuicompressor/yuicompressor.jar
assetic.less.node_bin     = <%= @assetic[:less][:node_bin] %>
assetic.less.node_paths   = <%= @assetic[:less][:node_paths] %>

; framework.xml
kernel.locale                = en
kernel.secret                = b011a4f1b18d2c8b01dcabdfbbfc09410
kernel.trusted_proxies       =
session.lifetime             = 86400
assets.base_urls             = 
assets.version               = <%= @assets[:version] %>

<% if @other.count %>
<% @other.each_pair do |name, value| %>
<%= name %> = <%= value %>
<% end%>
<% end %>