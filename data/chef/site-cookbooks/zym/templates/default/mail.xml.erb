<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://symfony.com/schema/dic/services
						http://symfony.com/schema/dic/services/services-1.0.xsd">
	<parameters>
		<parameter key="mail.transport"><%= @transport %></parameter>
		<parameter key="mail.encryption"><%= @encryption %></parameter>
		<parameter key="mail.auth_mode"><%= @auth_mode %></parameter>
		<parameter key="mail.host"><%= @host %></parameter>
		<parameter key="mail.port"><%= @port %></parameter>
		<parameter key="mail.username"><%= @username %></parameter>
		<parameter key="mail.password"><%= @password %></parameter>
	</parameters>
</container>